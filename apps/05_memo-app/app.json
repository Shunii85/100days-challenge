{
  "expo": {
    "name": "05_memo-app",
    "slug": "05_memo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "05memoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
